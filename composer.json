{
    "name": "bupy7/yii2-activerecord-history",
    "description": "This extension adds storage history of changes to the ActiveRecord model.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "module",
        "activerecord",
        "history",
        "behavior",
        "changes",
        "model"
    ],
    "license": "BSD-3-Clause",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 <6.0.0",
        "satooshi/php-coveralls": "^1.0.1",
        "friendsofphp/php-cs-fixer": "2.2.3"
    },
    "autoload": {
        "psr-4": {
            "bupy7\\activerecord\\history\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bupy7\\activerecord\\history\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
